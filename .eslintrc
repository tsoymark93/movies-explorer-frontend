{
  "env": {
    "browser": true,
    // "commonjs": true,
    "es2021": true
  },
  "extends": [
    // "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "prefer-promise-reject-errors": "off"
  }
}